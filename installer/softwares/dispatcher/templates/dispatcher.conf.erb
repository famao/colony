[DEFAULT]
bind_ip = <%= bind_ip %>
bind_port = <%= bind_port %>
user = <%= user %>
log_facility = <%= log_facility %>
log_name = <%= log_name %>

[pipeline:main]
pipeline = dispatcher

[app:dispatcher]
use = egg:dispatcher#dispatcher
conn_timout = 0.5
timout = 60
relay_rule = <%= relay_rule %>

[filter:swift3]
use = egg:dispatcher#swift3_for_colony

[filter:s3token]
use = egg:keystone#s3token
auth_protocol = <%= auth_protocol %>
auth_host = <%= auth_host %>
auth_port = <%= auth_port %>
